  },
  "shadowsocks_enabled": {
    "name": "Enable Shadowsocks Proxy",
    "type": "boolean",
    "default": false
  },
  "shadowsocks_port": {
    "name": "Shadowsocks Port",
    "type": "integer",
    "default": 8388,
    "minimum": 1024,
    "maximum": 65535
  },
  "shadowsocks_password": {
    "name": "Shadowsocks Password",
    "type": "string",
    "required": false,
    "placeholder": "Leave empty for default password"
  },
  "shadowsocks_method": {
    "name": "Shadowsocks Encryption Method",
    "type": "select",
    "options": [
      {"label": "AES-256-GCM", "value": "aes-256-gcm"},
      {"label": "AES-128-GCM", "value": "aes-128-gcm"},
      {"label": "ChaCha20-Poly1305", "value": "chacha20-poly1305"},
      {"label": "AES-256-CFB", "value": "aes-256-cfb"},
      {"label": "AES-128-CFB", "value": "aes-128-cfb"}
    ],
    "default": "aes-256-gcm"
  },
  "http_proxy_enabled": {
    "name": "Enable HTTP Proxy",
    "type": "boolean",
    "default": false
  },
  "http_proxy_port": {
    "name": "HTTP Proxy Port",
    "type": "integer",
    "default": 8888,
    "minimum": 1024,
    "maximum": 65535
  },
  "dns_providers": {
    "name": "DNS Providers",
    "type": "string",
    "required": false,
    "placeholder": "e.g., cloudflare,google,quad9"
  },
  "tz": {
    "name": "Timezone",
    "type": "string",
    "required": false,
