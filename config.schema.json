  "vpn_type": {
    "name": "VPN Type",
    "type": "select",
    "options": [
      {"label": "OpenVPN", "value": "openvpn"},
      {"label": "Wireguard", "value": "wireguard"}
    ],
    "default": "openvpn"
  },
  "openvpn_user": {
    "name": "OpenVPN Username",
    "type": "string",
